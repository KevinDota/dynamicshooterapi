extends default

block header

block content
  div.profile
    div.profile-image(style=`background: url('/images/${enemy.image}') top center
    no-repeat; background-size: cover;`)
    div.profile-details
      h1.profile-name
        | #{enemy.name}
      h2.profile-tagline
        | His Health: #{enemy.health}
      p.profile-bio
        | PositionX: #{enemy.positionX} PositionY:#{enemy.positionY}